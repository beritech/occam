r\n    </div>\r\n</template>\r\n<script>\r\n    import Formulario from \"./Formulario\";\r\n    import TablaFactura from \"./TablaFactura\";\r\n    // import TablaFactura from \"./TablaFactura\";\r\n    export default {\r\n        name: 'CrearFactura',\r\n        data: function () {\r\n            return {\r\n                serverURL: process.env.SERVER_URL,\r\n                factura: { nombre: '' },\r\n                facturas: []\r\n            }\r\n        },\r\n        components: {\r\n            TablaFactura,\r\n            Formulario\r\n            // TablaFactura\r\n        },\r\n        created () {\r\n            this.fetchData()\r\n        },\r\n        methods: {\r\n            fetchData: async function () {\r\n                try {\r\n                    await Promise.all([\r\n                        this.fetchFacturas()\r\n                    ])\r\n                } catch (error) {\r\n                    console.log(error)\r\n                }\r\n            },\r\n            fetchFacturas: function () {\r\n                fetch(`http://localhost:8080/facturacionFactura`)\r\n                    .then(r => r.json())\r\n                    .then(json => { this.facturas = json })\r\n                    .catch(error => console.error('Error retrieving vehicles: ' + error))\r\n            },\r\n            submitNewFactura () {\r\n                const factura = this.factura\r\n                console.log(factura);\r\n                fetch(`http://localhost:8080/facturacionFactura`, {\r\n                    method: 'POST',\r\n                    headers: {'Content-Type': 'application/json'},\r\n                    body: JSON.stringify(factura)\r\n                }).then(r => r.json())\r\n                    .then(json => {\r\n                        // this.factura.push(json)\r\n                        console.log(json)\r\n                        this.factura = { nombre: '' }\r\n                        this.fetchData()\r\n                    }).catch(ex => console.error('error', ex))\r\n            }\r\n        }\r\n    }\r\n\r\n</script>\r\n"],"sourceRoot":"src/components/Project/Facturacion/Factura"}]}